<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd" logicalFilePath="liquibaseChangeSet/use.xml">
    <changeSet author="mhines" id="add_use_table">
        <createTable tableName="use">
            <column name="id" type="INT4">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="rank" type="INT4" />
			<column name="name" type="text" />
			<column name="code" type="text" />
        </createTable>
        <addForeignKeyConstraint baseColumnNames="id" baseTableName="use" constraintName="fk_use_id" deferrable="false" initiallyDeferred="false" onDelete="CASCADE" onUpdate="NO ACTION" referencedColumnNames="use_id" referencedTableName="monitoring_location"/>
    </changeSet>
</databaseChangeLog>