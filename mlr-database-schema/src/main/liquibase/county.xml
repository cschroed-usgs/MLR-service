<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd" logicalFilePath="liquibaseChangeSet/county.xml">
    <changeSet author="mhines" id="add_county_table">
        <createTable tableName="county">
            <column name="id" type="INT4">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="name" type="text" />
			<column name="code" type="text" />
        </createTable>
        <addForeignKeyConstraint baseColumnNames="id" baseTableName="county" constraintName="fk_county_id" deferrable="false" initiallyDeferred="false" onDelete="CASCADE" onUpdate="NO ACTION" referencedColumnNames="county_id" referencedTableName="monitoring_location"/>
    </changeSet>
</databaseChangeLog>